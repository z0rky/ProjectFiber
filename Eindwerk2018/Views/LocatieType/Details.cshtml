@model Eindwerk2018.Models.LocatieType

@{
    ViewBag.Title = Eindwerk2018.Resources.Resource.LinkDetails;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Eindwerk2018.Resources.Resource.LinkDetails</h2>

<div>
    <h4>@Eindwerk2018.Resources.Resource.LocationTypeTitle</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.NaamNL)</dt>
        <dd>@Html.DisplayFor(model => model.NaamNL)</dd>

        <dt>@Html.DisplayNameFor(model => model.NaamFR)</dt>
        <dd>@Html.DisplayFor(model => model.NaamFR)</dd>

        <dt>@Html.DisplayNameFor(model => model.DescNL)</dt>
        <dd>@Html.DisplayFor(model => model.DescNL)</dd>

        <dt>@Html.DisplayNameFor(model => model.DescFR)</dt>
        <dd>@Html.DisplayFor(model => model.DescFR)</dd>
    </dl>
</div>
<p>
    <input type="button" class="btn btn-primary" value=@Eindwerk2018.Resources.Resource.LinkBack onclick="history.go(-1);" />
    <button type="button" class="btn btn-primary" onclick="location.href='@Url.Action("Edit","LocatieType", new {id = Model.Id })'">@Eindwerk2018.Resources.Resource.LinkEdit</button>
</p>
